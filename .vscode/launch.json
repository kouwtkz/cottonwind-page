{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "dev",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev"
    },
    {
      "name": "Launch Chrome",
      "type": "chrome",
      "url": "http://localhost:51730",
      "request": "launch",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "Launch Firefox",
      "type": "firefox",
      "request": "launch",
      "reAttach": true,
      "url": "http://localhost:51730"
    },
    {
      "name": "Launch Chrome 2",
      "type": "chrome",
      "url": "http://localhost:51740",
      "request": "launch",
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ]
    },
    {
      "name": "dev-calendar",
      "type": "node-terminal",
      "request": "launch",
      "command": "npm run dev-calendar"
    }
  ],
  "compounds": [
    {
      "name": "Chrome dev",
      "configurations": ["Launch Chrome", "dev"]
    },
    {
      "name": "Chrome dev-calendar",
      "configurations": ["Launch Chrome 2", "dev-calendar"]
    }
  ]
}
