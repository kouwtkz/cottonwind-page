{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-tap-highlight-color",
          "min-width: fit-content",
          "overscroll-behavior",
          "scrollbar-color",
          "scrollbar-width"
        ]
      }
    ],
    "no-inline-styles": "off",
    "create-element-svg": "off",
    "typescript-config/is-valid": "off",
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off"
      }
    ],
    "detect-css-reflows/composite": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 119",
    "not chrome <= 119",
    "not edge <= 119",
    "not ios_saf <= 17.1",
    "not opera <= 103",
    "not safari <= 17.1",
    "not samsung <= 23"
  ]
}