{
  "name": "my-react-router-app-3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && bun run vite build --mode css && bun run vite build --mode worker",
    "cf-typegen": "wrangler types --strict-vars false",
    "deploy": "bun run build && wrangler deploy",
    "start": "bun run build && bun run start-only",
    "start-only": "wrangler dev --test-scheduled",
    "dev": "bun run vite dev --host",
    "dev-calendar": "bun run vite dev --mode dev-calendar",
    "build-calendar": "bun run vite build --mode calendar && bun run vite build --mode calendar-css && bun run vite build --mode calendar-beforeClient && bun run vite build --mode calendar-worker",
    "deploy-calendar": "bun run build-calendar && wrangler deploy -c dist/calendar/wrangler.json",
    "start-calendar": "bun run build-calendar && bun run start-only-calendar",
    "start-only-calendar": "wrangler dev -c dist/calendar/wrangler.json",
    "postinstall": "bun run cf-typegen",
    "typecheck": "bun run cf-typegen && react-router typegen && tsc -b",
    "update": "ncu -u && bun update"
  },
  "dependencies": {
    "@dnd-kit/sortable": "^10.0.0",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/google-calendar": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/list": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "@hookform/resolvers": "^5.2.2",
    "html-react-parser": "^5.2.11",
    "image-dimensions": "^2.5.0",
    "isbot": "^5.1.32",
    "marked": "^17.0.1",
    "mp3tag.js": "^3.14.1",
    "react": "^19.2.3",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.70.0",
    "react-hotkeys-hook": "^5.2.1",
    "react-icons": "^5.5.0",
    "react-router": "^7.11.0",
    "react-select": "^5.10.2",
    "react-slider": "^2.0.6",
    "react-swipeable": "^7.0.2",
    "react-toastify": "^11.0.5",
    "rss": "^1.2.2",
    "schema-dts": "^1.1.5",
    "tsqlstring": "^1.0.1",
    "xmldom": "^0.6.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.19.0",
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/react-slider": "^1.3.6",
    "@types/rss": "^0.0.32",
    "@types/tsqlstring": "^1.0.2",
    "@types/xmldom": "^0.1.34",
    "better-sqlite3": "^12.5.0",
    "sass": "^1.97.2",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "wrangler": "^4.56.0"
  }
}
