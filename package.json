{
  "type": "module",
  "scripts": {
    "dev": "npx vite --host --port 51730",
    "dev-media": "npx vite --host --port 51731 --mode media",
    "dev-ssl": "npx vite --host --port 51730 --mode=ssl",
    "dev-media-ssl": "npx vite --host --port 51731 --mode media-ssl",
    "build": "npx vite build --mode client-overwrite && npm run build-ssg && npm run build-worker",
    "build-client": "npx vite build --mode client",
    "build-ssg": "npx vite build --mode ssg",
    "build-worker": "npx vite build",
    "start": "npx wrangler pages dev --port 51730",
    "deploy": "npm run build && npm run deploy-only",
    "deploy-only": "npx wrangler pages deploy dist --commit-dirty true --branch master",
    "deploy-sv": "npx wrangler deploy -c wrangler-sv.toml",
    "delete-all-image": "curl -X DELETE http://localhost:51730/api/image/all",
    "delete-all-character": "curl -X DELETE http://localhost:51730/api/character/all",
    "delete-all-blog": "curl -X DELETE http://localhost:51730/api/blog/all",
    "clean-vite-timestamp": "node scripts/clean-vite-timestamp.cjs"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@hookform/resolvers": "^3.9.0",
    "@vitejs/plugin-react": "^4.3.1",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "epubjs": "^0.3.93",
    "highlight.js": "^11.10.0",
    "hono": "^4.6.3",
    "html-react-parser": "^5.1.16",
    "iconv-lite": "^0.6.3",
    "image-dimensions": "^2.3.0",
    "image-meta": "^0.2.1",
    "jotai": "^2.10.0",
    "js-yaml": "^4.1.0",
    "jsonc-parser": "^3.3.1",
    "marked": "^14.1.2",
    "music-metadata": "^10.5.0",
    "react": "^18.3.1",
    "react-comic-viewer": "^0.4.0",
    "react-cookie": "^7.2.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.53.0",
    "react-hotkeys-hook": "^4.5.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2",
    "react-select": "^5.8.1",
    "react-toastify": "^10.0.5",
    "react-transition-group": "^4.4.5",
    "schema-dts": "^1.1.2",
    "tsqlstring": "^1.0.1",
    "zustand": "^5.0.0-rc.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240925.0",
    "@hono/vite-cloudflare-pages": "^0.4.2",
    "@hono/vite-dev-server": "^0.16.0",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.11",
    "@types/rss": "^0.0.32",
    "@types/tsqlstring": "^1.0.2",
    "@types/xmldom": "^0.1.34",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "buffer": "^6.0.3",
    "clui": "^0.3.6",
    "npm-run-all": "^4.1.5",
    "sass": "^1.79.3",
    "tsx": "^4.16.2",
    "vite": "^5.4.8",
    "vite-plugin-sitemap": "^0.7.1",
    "vite-tsconfig-paths": "^5.0.1",
