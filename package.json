{
  "name": "my-react-router-app-3",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build && npx vite build --mode css && npx vite build --mode worker",
    "cf-typegen": "wrangler types --strict-vars false",
    "deploy": "npm run build && wrangler deploy",
    "start": "npm run build && npm run start-only",
    "start-only": "wrangler dev --test-scheduled",
    "dev": "npx vite dev",
    "dev-calendar": "npx vite dev --mode dev-calendar",
    "build-calendar": "npx vite build --mode calendar && npx vite build --mode calendar-css && npx vite build --mode calendar-beforeClient && npx vite build --mode calendar-worker",
    "deploy-calendar": "npm run build-calendar && wrangler deploy -c dist/calendar/wrangler.json",
    "start-calendar": "npm run build-calendar && npm run start-only-calendar",
    "start-only-calendar": "wrangler dev -c dist/calendar/wrangler.json",
    "postinstall": "npm run cf-typegen",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b"
  },
  "dependencies": {
    "@dnd-kit/sortable": "^10.0.0",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/google-calendar": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/list": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@hookform/resolvers": "^5.1.1",
    "html-react-parser": "^5.2.5",
    "image-dimensions": "^2.3.0",
    "isbot": "^5.1.28",
    "marked": "^15.0.12",
    "mp3tag.js": "^3.14.1",
    "react": "^19.1.0",
    "react-device-detect": "^2.2.3",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.57.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-icons": "^5.5.0",
    "react-router": "^7.6.2",
    "react-select": "^5.10.1",
    "react-slider": "^2.0.6",
    "react-swipeable": "^7.0.2",
    "react-toastify": "^11.0.5",
    "rss": "^1.2.2",
    "schema-dts": "^1.1.5",
    "tsqlstring": "^1.0.1",
    "xmldom": "^0.6.0",
    "zod": "^3.25.63",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.5.1",
    "@react-router/dev": "^7.6.2",
    "@tailwindcss/vite": "^4.1.10",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-slider": "^1.3.6",
    "@types/rss": "^0.0.32",
    "@types/tsqlstring": "^1.0.2",
    "@types/xmldom": "^0.1.34",
    "better-sqlite3": "^11.10.0",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.19.2"
  },
  "overrides": {
    "react-slider": {
      "react": "^19"
    }
  }
}
